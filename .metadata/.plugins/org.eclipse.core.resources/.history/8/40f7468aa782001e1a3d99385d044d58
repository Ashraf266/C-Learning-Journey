#include "gradebook.h"


GradeBook_v3::GradeBook_v3(string name)
{
	setCourseName(name);
}

void GradeBook_v3::setCourseName(string name)
{
	courseName = name;
}

string GradeBook_v3::getCourseName()
{
	return courseName;
}

void GradeBook_v3::displayMessage()
{
	cout << "Welcome to the Grade Book for " << courseName << "!" << endl;
}
